<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8F5CFF">

    <Button
        android:text="@string/startup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/startup"
        android:elevation="0dp"
        android:onClick="startupsignup"/>

    <Button
        android:text="@string/free_agent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/free_agent"
        android:elevation="0dp"
        android:onClick="free_agent" />

    <Button
        android:text="@string/consultant"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/consultant"
        android:onClick="consultantidentity"/>

</LinearLayout>